        "\n",
        "A = sp.Matrix([1, 2])\n",
        "\n",
        "m, b = symbols('m b')\n",
        "x, y = symbols('x y')\n",
        "\n",
        "eq1 = EnhancedEq(2*x + 3, y)\n",
        "\n",
        "m_sol = -1/2 #The line is parallel => the slope is\n",
        "\n",
        "eq2 = EnhancedEq(m_sol*1 + b, 2)\n",
        "\n",
        "b_sol = sp.solve(eq2, b)[0]\n",
        "\n",
        "LineEq = EnhancedEq(m_sol*x + b_sol, y)\n",
        "\n",
        "print(\"The equation of the line is\", LineEq)\n",
        "\n",
        "# Calculate the slope and y-intercept\n",
        "slope = m_sol\n",
        "y_intercept = b_sol\n",
