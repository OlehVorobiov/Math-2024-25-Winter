      "source": [
        "#We have the line  y=2x+3  and point  A(1,2) . Find the distance from point  A  to the line.\n",
        "\n",
        "eq1 = EnhancedEq(2*x + 3, y)\n",
        "\n",
        "A = sp.Matrix([1, 2])\n",
        "\n",
        "#Perpindicular line\n",
        "m_sol = -1/2\n",
        "\n",
        "b = A[1] - m_sol * A[0]\n",
        "\n",
        "LineEq = EnhancedEq(m_sol*x + b, y)\n",
        "\n",
        "print(LineEq)\n",
        "\n",
        "eq3 = LineEq*4 + eq1\n",
        "\n",
        "print(eq3)\n",
        "\n",
        "y_sol = sp.solve(eq3, y)[0]\n",
        "\n",
        "x_sol = (y_sol - b) / m_sol\n",
        "\n",
        "print(x_sol, y_sol)\n",
        "\n",
        "distance = sp.sqrt((A[0] - x_sol)**2 + (A[1] - y_sol)**2)\n",
        "\n",
        "print(\"The distance is \", distance)\n",
        "\n",
        "\n",
        "plt.plot(np.linspace(-5, 5, 100), 2*np.linspace(-5, 5, 100) + 3, label='y = 2x + 3')\n",
        "plt.plot(np.linspace(-5, 5, 100), -1/2*np.linspace(-5, 5, 100) + 2.5, label='y = -1/2x + 2.5')\n",
        "plt.plot(A[0], A[1], 'ro')  # 'ro' for red circles as markers\n",
        "\n",
        "plt.xlabel(\"X-axis\")\n",
        "plt.ylabel(\"Y-axis\")\n",
        "plt.title(\"Extended Line through a Point\")\n",
        "plt.grid(True)  # Add a grid for better visualization\n",
        "plt.gca().set_aspect('equal', adjustable='box')\n",
        "\n",
        "plt.xlim(min(-1, -1), max(6, 6))\n",
        "plt.ylim(min(-1, -1), max(6, 6))\n",
        "plt.legend()\n",
