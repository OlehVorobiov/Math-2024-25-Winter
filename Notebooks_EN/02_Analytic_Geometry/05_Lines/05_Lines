        "#Write the equation of the line passing through point  A(1,2)  and parallel to the vector  v⃗ =[2,3] .\n",
        "\n",
        "A = sp.Matrix([1, 2])\n",
        "v = sp.Matrix([2, 3])\n",
        "\n",
        "m = v[1] / v[0]\n",
        "b = A[1] - m * A[0]\n",
        "\n",
        "LineEq = EnhancedEq(m*x + b, y)\n",
        "\n",
        "print(LineEq)\n",
        "\n",
        "plt.plot(np.linspace(-5, 5, 100), 3/2*np.linspace(-5, 5, 100) + 1/2)\n",
        "plt.quiver(0, 0, float(v[0]), float(v[1]), angles='xy', scale_units='xy', scale=1, color='r', label='v')\n",
        "\n",
        "# Optionally, plot the original points\n",
        "plt.plot(A[0], A[1], 'ro')  # 'ro' for red circles as markers\n",
        "\n",
        "plt.xlabel(\"X-axis\")\n",
        "plt.ylabel(\"Y-axis\")\n",
        "plt.title(\"Extended Line through a Point\")\n",
        "plt.grid(True)  # Add a grid for better visualization\n",
        "plt.gca().set_aspect('equal', adjustable='box')\n",
        "\n",
        "plt.xlim(min(-1, -1), max(6, 6))\n",
        "plt.ylim(min(-1, -1), max(6, 6))\n",
