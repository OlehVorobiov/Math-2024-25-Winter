        "from operator import eq\n",
        "#We have the line  y=2x+3 . Find an example of a line perpendicular and parallel to it.\n",
        "\n",
        "eq1 = EnhancedEq(2*x + 3, y)\n",
        "\n",
        "m_sol = 2\n",
        "\n",
        "eq2 = EnhancedEq(m_sol*x + 4, y) #The line is parallel => b != 3\n",
        "eq3 = EnhancedEq(1/m_sol*x + 3, y)\n",
        "\n",
        "print(\"Parallel line:\", eq2)\n",
        "print(\"Perpindicular line:\", eq3)\n",
        "\n",
        "plt.plot(np.linspace(-5, 5, 100), 2*np.linspace(-5, 5, 100) + 3, label='y = 2x + 3')\n",
        "plt.plot(np.linspace(-5, 5, 100), m_sol*np.linspace(-5, 5, 100) + 4, label=f'y = {m_sol}x + 4')\n",
        "plt.plot(np.linspace(-5, 5, 100), -1/m_sol*np.linspace(-5, 5, 100) + 3, label=f'y = {-1/m_sol}x + 3')\n",
        "\n",
        "plt.xlabel(\"X-axis\")\n",
        "plt.ylabel(\"Y-axis\")\n",
        "plt.title(\"Extended Line through a Point\")\n",
        "plt.grid(True)  # Add a grid for better visualization\n",
        "plt.gca().set_aspect('equal', adjustable='box')\n",
        "\n",
        "plt.xlim(min(-1, -1), max(6, 6))\n",
        "plt.ylim(min(-1, -1), max(6, 6))\n",
        "plt.legend()\n",
        "plt.show()"
