        "#We have two lines  y=2x+3  and  y=3x+2 . Find the intersection point of these lines and calculate the angle between them.\n",
        "\n",
        "eq1 = EnhancedEq(2*x + 3, y)\n",
        "eq2 = EnhancedEq(3*x + 2, y)\n",
        "\n",
        "eq3 = eq1 - eq2\n",
        "\n",
        "print(eq3)\n",
        "\n",
        "x_sol = sp.solve(eq3, x)[0]\n",
        "\n",
        "y_sol = 2*x_sol + 3\n",
        "\n",
        "print(\"Coordinats of the intersection are\", \"x =\", x_sol, \"y =\", y_sol)\n",
        "A = sp.Matrix([x_sol, y_sol])\n",
        "\n",
        "angle_rad = np.arctan(abs((2 - 3) / (1 + 2 * 3)))\n",
        "angle_degrees = np.degrees(angle_rad)\n",
        "print(\"angle between the lines is \", angle_degrees)\n",
        "\n",
        "plt.plot(np.linspace(-5, 5, 100), 2*np.linspace(-5, 5, 100) + 3)\n",
        "plt.plot(np.linspace(-5, 5, 100), 3*np.linspace(-5, 5, 100) + 2)\n",
        "\n",
        "\n",
        "# Optionally, plot the original points\n",
        "plt.plot(A[0], A[1], 'ro')  # 'ro' for red circles as markers\n",
        "\n",
        "plt.xlabel(\"X-axis\")\n",
        "plt.ylabel(\"Y-axis\")\n",
        "plt.title(\"Extended Line through a Point\")\n",
        "plt.grid(True)  # Add a grid for better visualization\n",
        "plt.gca().set_aspect('equal', adjustable='box')\n",
        "\n",
        "plt.xlim(min(-1, -1), max(6, 6))\n",
        "plt.ylim(min(-1, -1), max(6, 6))\n",
