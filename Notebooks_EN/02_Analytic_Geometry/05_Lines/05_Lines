      "cell_type": "code",
      "source": [
        "#The line intersects the coordinate axes at points  A(2,0)  and  B(0,3) . Find the equation of the line.\n",
        "\n",
        "A = sp.Matrix([2, 0])\n",
        "B = sp.Matrix([0, 3])\n",
        "\n",
        "x, y, m, b = sp.symbols(\"x y m b\")\n",
        "\n",
        "eq1 = EnhancedEq(m*2 + b, 0)\n",
        "eq2 = EnhancedEq(m*0 + b, 3)\n",
        "\n",
        "b_sol = sp.solve(eq2, b)[0]\n",
        "\n",
        "eq3 = eq1 - eq2\n",
        "\n",
        "m_sol = sp.solve(eq3, m)[0]\n",
        "\n",
        "LineEq = EnhancedEq(m_sol*x + b_sol, y)\n",
        "\n",
        "print(\"The equation of the line:\", LineEq)\n",
        "\n",
        "plt.plot(np.linspace(-5, 5, 100), -3/2*np.linspace(-5, 5, 100) + 3, label='y = -1.5x + 3')\n",
        "plt.plot(A[0], A[1], 'ro')  # 'ro' for red circles as markers\n",
